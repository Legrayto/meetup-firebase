User\">\n            <v-card-text>\n              <v-row>\n                <v-col class=\"d-flex\">\n                  <v-avatar\n                    color=\"grey lighten-4\"\n                    size=\"100\"\n                    class=\"elevation-3\"\n                  >\n                    <v-img :src=\"user.avatar\" v-if=\"user.avatar\"></v-img>\n                    <v-icon v-else large color=\"grey lighten-1\"\n                      >mdi-plus</v-icon\n                    >\n                  </v-avatar>\n                  <v-spacer></v-spacer>\n                  <app-edit-profile\n                    :user=\"{ name: user.name, avatar: user.avatar }\"\n                  ></app-edit-profile>\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col cols=\"12\">\n                  <div class=\"text-h6 text-sm-h4 font-weight-bold\">\n                    {{ user.name }}\n                  </div>\n                  <div class=\"text-subtitle font-italic indigo--text\">\n                    {{ user.email }}\n                  </div>\n                </v-col>\n              </v-row>\n            </v-card-text>\n          </v-card>\n        </v-col>\n      </v-row>\n    </div>\n  </v-container>\n</template>\n\n<script>\nexport default {\n  name: \"Profile\",\n  computed: {\n    loading() {\n      return this.$store.getters.loading;\n    },\n    user() {\n      return this.$store.getters.user;\n    },\n    isNewUser() {\n      return !this.loaging && !this.user.avatar && !this.user.name;\n    },\n  },\n};\n</script>\n"]}]}